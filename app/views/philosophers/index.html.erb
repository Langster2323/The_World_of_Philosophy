<script src="//maps.google.com/maps/api/js?v=3.23&key=[your API key]"></script>
<script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>
<script src='//cdn.rawgit.com/printercu/google-maps-utility-library-v3-read-only/master/infobox/src/infobox_packed.js' type='text/javascript'></script> <!-- only if you need custom infoboxes -->
<div>
  <h1>Listing Philosophers</h1>
</div>
<div class="row">
  <div class="card file-card col m8 m2">
    <div class="card-content">
      <table>
        <thead>
          <tr>
            <th>Latitude</th>
            <th>Longitude</th>
            <th>Address</th>
            <th>Name</th>
            <th>Birthdate</th>
            <th>Concentration</th>
            <th>Wikipedia</th>
          </tr>
        </thead>

        <tbody>
          <%= content_tag_for(:tr, @philosophers) do |philosopher| %>
            <td><%= philosopher.latitude %></td>
            <td><%= philosopher.longitude %></td>
            <td><%= philosopher.address %></td>
            <td><%= philosopher.name %></td>
            <td><%= philosopher.born %> </td>
            <td><%= philosopher.concentration %></td>
            <td><%= philosopher.wikipedia %></td>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
